import java.util.*;
public class JavaBasics {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
System.out.print("Enter the nos You want to Add");
      int a = sc.nextInt();
      int b = sc.nextInt();
      int sum = a+b;
      System.out.println("Your sum will be");
      System.out.println(sum);
    


    }


}
